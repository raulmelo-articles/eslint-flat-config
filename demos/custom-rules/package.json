{
  "name": "custom-rules",
  "version": "1.0.0",
  "description": "Example of how to use custom rules in ESLint flat config",
  "scripts": {
    "lint": "eslint .",
    "lint:eslintrc": "ESLINT_USE_FLAT_CONFIG=false eslint . --rulesdir eslint/rules"
  },
  "devDependencies": {
    "eslint": "8.45.0"
  }
}
